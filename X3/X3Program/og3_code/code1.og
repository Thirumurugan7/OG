FUN counter(number)
    # Base case: if number is less than 10, return number
    IF number < 10 THEN
        RETURN number
    # Recursive case: if number is 10 or greater, call 'counter' recursively with number - 1
    ELSE
        RETURN counter(number - 1)
    END
END

# Main program
# Loop from 1 to 10
FOR i = 1 TO 10 THEN
    # Print the count using 'counter' function
    PRINT(counter(i))
END